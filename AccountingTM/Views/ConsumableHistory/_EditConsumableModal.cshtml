@* Изменение информации о расходном материале *@
<div class="modal fade" tabindex="-1" id="EditConsumableModal">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title"><i class="fa-solid fa-pen me-2"></i> Изменение данных расходного материала</h5>
				<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
			</div>
			<div class="modal-body">
				<div class="mb-3">
					<label class="form-label">Наименование типа</label>
					<select id="typeConsumable" class="form-control"></select>
				</div>

				<div class="mb-3">
					<label class="form-label">Наименование бренда</label>
					<select id="brand" class="form-control"></select>
				</div>

				<div class="mb-3">
					<label class="form-label">Наименование модели</label>
					<input type="text" class="form-control" id="model" name="model" placeholder="Введите модель">
				</div>

				<div class="mb-3">
					<label class="form-label">Местоположение</label>
					<select id="location" class="form-control"></select>
				</div>

				<div class="mb-3">
					<label class="form-label">Единица измерения</label>
					<select id="unit" class="form-control"></select>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Закрыть</button>
				<button id="editConsumableBtn" class="btn btn-primary"><i class="fa-solid fa-save"></i> Изменить</button>
			</div>
		</div>
	</div>
</div>