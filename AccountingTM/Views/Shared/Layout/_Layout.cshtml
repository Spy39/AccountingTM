<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - WebApplication1</title>
	<style>

	</style>
	@await Html.PartialAsync("_Styles.cshtml")
	@await RenderSectionAsync("Styles", required: false)
</head>
<body class="sidebar-mini layout-fixed" style="height: auto;">
	<div class="wrapper">
		<!-- Navbar -->
		<nav class="main-header navbar navbar-expand navbar-white navbar-light">
			@await Html.PartialAsync("_Header.LeftNavbar.cshtml")
			@await RenderSectionAsync("LeftNavbar", required: false)
			@await Html.PartialAsync("_Header.RightNavbar.cshtml")
		</nav>
		<!-- /.navbar -->
		@await Html.PartialAsync("_SideBar.cshtml")
		<div class="content-wrapper">
			@RenderBody()
		</div>
		@await Html.PartialAsync("_Footer.cshtml")
		<!-- Preloader -->
	</div>
	@await Html.PartialAsync("_Scripts.cshtml")
	@await RenderSectionAsync("Scripts", required: false)
</body>

</html>
